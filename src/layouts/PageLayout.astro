---
import BaseLayout from "./BaseLayout.astro";

interface Props {
  frontmatter: {
    title: string;
  };
}

const { frontmatter } = Astro.props;
---

<BaseLayout title={frontmatter.title}>
  <article class="mx-4 sm:mx-auto py-12 md:py-20 prose prose-blue lg:prose-lg">
   <slot />
  </article>
</BaseLayout>
